{
  "displayName": "Homebridge SensorPush",
  "name": "homebridge-sensorpush",
  "version": "1.0.0",
  "description": "A Homebridge plugin for SensorPush temperature and humidity sensors",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/USERNAME/homebridge-sensorpush.git"
  },
  "bugs": {
    "url": "https://github.com/USERNAME/homebridge-sensorpush/issues"
  },
  "engines": {
    "node": ">=18.0.0",
    "homebridge": ">=1.6.0"
  },
  "main": "dist/index.js",
  "scripts": {
    "lint": "eslint src/**.ts --max-warnings=0",
