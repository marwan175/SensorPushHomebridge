  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/node-persist": "^3.1.8",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.54.0",
    "homebridge": "^1.7.0",
    "nodemon": "^3.0.2",
    "rimraf": "^5.0.5",
    "typescript": "^5.3.3"
